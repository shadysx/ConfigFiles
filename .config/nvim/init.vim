source $HOME/.config/nvim/vim-plug/plugins.vim

:set number
:set autoindent
:set tabstop=4
:set shiftwidth=4
:set smarttab
:set softtabstop=4
:set mouse=a

:inoremap kj <esc>

:hi StatusLine ctermbg=NONE cterm=NONE

" How To Do 90 % of What Plugins Do (youtube)

set nocompatible 
syntax enable
filetype plugin on

